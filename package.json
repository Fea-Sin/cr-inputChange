{
  "name": "cr-inputchange",
  "version": "1.1.0",
  "description": "a react component about input",
  "scripts": {
    "start": "rc-tools run server",
    "build": "rc-tools run build",
    "lint": "rc-tools run lint",
    "compile": "rc-tools run compile --babel-runtime",
    "pub": "npm run test && npm run build && npm run compile && npm publish",
    "test": "jest --no-cache --updateSnapshot",
    "coverage": "jest --coverage "
  },
  "keywords": [],
  "author": "feasin",
  "config": {
    "port": 8018,
    "entry": {
      "cr-comp": [
        "./assets/index.less",
        "./src/index.js"
      ]
    }
  },
  "files": [
    "dist",
    "es",
    "lib",
    "assets/*.css"
  ],
  "main": "./lib/index",
  "module": "./es/index",
  "jest": {
    "setupFiles": [
      "./tests/setup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*",
      "!src/utils/**/*"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "transform": {
      "\\.jsx?$": "./node_modules/rc-tools/scripts/jestPreprocessor.js"
    },
    "verbose": true,
    "testURL": "http://localhost/"
  },
  "devDependencies": {
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "enzyme-to-json": "^3.2.1",
    "jest": "^22.0.0",
    "pre-commit": "1.x",
    "rc-tools": "^9.5.3"
  },
  "dependencies": {
    "antd": "3.17.0",
    "babel-runtime": "6.x",
    "classnames": "2.x",
    "prop-types": "^15.5.6",
    "rc-util": "^4.1.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "license": "MIT"
}
